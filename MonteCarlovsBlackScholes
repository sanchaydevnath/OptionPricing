      ],
      "source": [
        "# Monte Carlo European Call Pricer + Black–Scholes benchmark\n",
        "# Robust to yfinance's auto_adjust change and missing data issues\n",
        "\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Optional: install yfinance if missing (uncomment the next two lines in Colab)\n",
        "# import sys, subprocess\n",
        "# subprocess.check_call([sys.executable, \"-m\", \"pip\", \"install\", \"-q\", \"yfinance\"])\n",
        "\n",
        "import yfinance as yf\n",
        "from scipy.stats import norm\n",
        "\n",
        "# ------------------------------\n",
        "# 0) Helper functions\n",
        "# ------------------------------\n",
        "def black_scholes_call(S, K, T, r, sigma):\n",
        "    \"\"\"Closed-form BS price of a European call.\"\"\"\n",
        "    if T <= 0 or sigma <= 0 or S <= 0 or K <= 0:\n",
        "        return max(S - K, 0.0)\n",
        "    d1 = (np.log(S / K) + (r + 0.5 * sigma**2) * T) / (sigma * np.sqrt(T))\n",
        "    d2 = d1 - sigma * np.sqrt(T)\n",
        "    return S * norm.cdf(d1) - K * np.exp(-r * T) * norm.cdf(d2)\n",
        "\n",
        "def monte_carlo_call(S, K, T, r, sigma, n_sim=100_000, antithetic=True, seed=42):\n",
        "    \"\"\"Monte Carlo price for a European call under risk-neutral GBM.\n",
        "       Returns (price, standard_error).\"\"\"\n",
        "    rng = np.random.default_rng(seed)\n",
        "    m = n_sim if not antithetic else n_sim // 2\n",
        "\n",
        "    Z = rng.standard_normal(m)\n",
        "    if antithetic:\n",
        "        Z = np.concatenate([Z, -Z])  # variance reduction\n",
        "\n",
        "    ST = S * np.exp((r - 0.5 * sigma**2) * T + sigma * np.sqrt(T) * Z)\n",
        "    payoffs = np.maximum(ST - K, 0.0)\n",
        "    disc = np.exp(-r * T)\n",
        "\n",
        "    price = disc * payoffs.mean()\n",
        "    # MC standard error of the discounted mean\n",
        "    se = disc * payoffs.std(ddof=1) / np.sqrt(payoffs.size)\n",
        "    return price, se\n",
        "\n",
        "# ------------------------------\n",
        "# 1) Fetch stock data\n",
        "# ------------------------------\n",
        "ticker = \"AAPL\"                      # change as you like\n",
        "data = yf.download(ticker, start=\"2024-01-01\", end=\"2025-01-01\", auto_adjust=True)\n",
        "\n",
        "if data.empty:\n",
        "    raise ValueError(\"No data downloaded. Check ticker or date range.\")\n",
        "\n",
        "# yfinance (auto_adjust=True) returns ['Open','High','Low','Close','Volume']\n",
        "# Use Close, which is already adjusted.\n",
        "close = data[\"Close\"].dropna().astype(float)\n",
        "S0 = float(close.iloc[-1])\n",
        "print(f\"Current {ticker} price (Close, adjusted): {S0:.2f}\")\n",
        "\n",
        "# ------------------------------\n",
        "# 2) Parameters\n",
        "# ------------------------------\n",
        "K = 200.0      # strike\n",
        "T = 1.0        # years to expiry\n",
        "r = 0.05       # annual risk-free (cont. comp.)\n",
        "\n",
        "# Historical volatility estimate (annualized) from daily log returns\n",
        "daily_log_ret = np.log(close / close.shift(1)).dropna()\n",
        "sigma = float(daily_log_ret.std(ddof=1) * np.sqrt(252))  # 252 trading days\n",
        "print(f\"Estimated annual volatility (hist): {sigma:.4f}\")\n",
        "\n",
        "# ------------------------------\n",
        "# 3) Monte Carlo vs Black–Scholes\n",
        "# ------------------------------\n",
        "mc_price, mc_se = monte_carlo_call(S0, K, T, r, sigma, n_sim=200_000, antithetic=True, seed=123)\n",
        "bs_price = black_scholes_call(S0, K, T, r, sigma)\n",
        "\n",
        "print(f\"Monte Carlo price: {mc_price:.4f}  (± {1.96*mc_se:.4f} at 95% CI)\")\n",
        "print(f\"Black–Scholes price: {bs_price:.4f}\")\n",
        "\n",
        "# ------------------------------\n",
        "# 4) Convergence plot\n",
        "# ------------------------------\n",
        "sim_counts = np.unique(np.logspace(2, 6, 12, dtype=int))  # 1e2 ... 1e6\n",
        "mc_estimates = []\n",
        "for n in sim_counts:\n",
        "    est, _ = monte_carlo_call(S0, K, T, r, sigma, n_sim=n, antithetic=True, seed=123)\n",
        "    mc_estimates.append(est)\n",
        "\n",
        "plt.figure(figsize=(8,5))\n",
        "plt.plot(sim_counts, mc_estimates, marker='o', label=\"Monte Carlo\")\n",
        "plt.axhline(bs_price, linestyle=\"--\", label=\"Black–Scholes\")\n",
        "plt.xscale(\"log\")\n",
        "plt.xlabel(\"Number of simulations (log scale)\")\n",
        "plt.ylabel(\"Call price\")\n",
        "plt.title(f\"Monte Carlo Convergence ({ticker})\")\n",
        "plt.legend()\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# ------------------------------\n",
        "# 5) Simulated terminal distribution (histogram)\n",
        "# ------------------------------\n",
        "# Generate one big set for visualization\n",
        "N = 200_000\n",
        "rng = np.random.default_rng(7)\n",
        "Z = rng.standard_normal(N)\n",
        "ST = S0 * np.exp((r - 0.5 * sigma**2) * T + sigma * np.sqrt(T) * Z)\n",
        "\n",
        "plt.figure(figsize=(8,5))\n",
        "plt.hist(ST, bins=100, density=True, alpha=0.6)\n",
        "plt.axvline(K, linestyle=\"--\", label=f\"Strike K={K:.0f}\")\n",
        "plt.xlabel(\"Terminal stock price $S_T$\")\n",
        "plt.ylabel(\"Density\")\n",
        "plt.title(f\"Simulated $S_T$ Distribution in {int(T)} Year ({ticker})\")\n",
        "plt.legend()\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ]
    }
  ]
}
